[Unit]
Description=uWSGI service
After=syslog.target

[Service]
ExecStart=/home/ec2-user/myvenv/bin/uwsgi -ini /srv/django_rest/.config/uwsgi/restfulserver.ini

RuntimeDirectory=uwsgi
Restart=always
KillSignal=SIGQUIT
Type=notify
StandardError=syslog
NotifyAccess=all

[Install]
WantedBy=multi-user.target